# /etc/profile


export PS1='\[\e[1;31m\]\h \w # \[\e[0m\]\[\e[32m\]\[\e[00m\]'
export TERM='linux'
export TERMINFO=/etc/terminfo

alias ms='/sbin/mount_sdcard'
alias us='/sbin/umount_sdcard'

echo "You can run resize command to make the terminal output match the terminal size" > /dev/console
hostname initramfs
resize
cd /
echo
